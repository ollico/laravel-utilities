{
    "name": "ollico/laravel-utilities",
    "description": "A Laravel utilities package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David Bonner",
            "email": "david@ollicosoftware.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "davidianbonner/enumerated": "4.0.2"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0"
    },
        "extra": {
        "laravel": {
            "providers": [
                "Ollico\\Utilities\\ServiceProviders\\LaravelUtilitiesServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Ollico\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ollico\\Utilities\\Tests\\": "tests/"
        }
    }
}
