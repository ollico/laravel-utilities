{
  "name": "ollico/laravel-utilities",
  "description": "A Laravel utilities package.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "David Bonner",
      "email": "david@ollicosoftware.com"
    },
    {
      "name": "Keiron McGivern",
      "email": "keironmcgivern@ollicosoftware.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/contracts" : "^9.30|^10.0",
    "illuminate/support": "^9.30|^10.0",
    "illuminate/database": "^9.30|^10.0",
    "davidianbonner/enumerated": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ollico\\Utilities\\ServiceProviders\\LaravelUtilitiesServiceProvider"
      ],
      "aliases": {}
    }
  },
  "autoload": {
    "psr-4": {
      "Ollico\\Utilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ollico\\Utilities\\Tests\\": "tests/"
    }
  }
}
